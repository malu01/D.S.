#include<stdio.h>
int mss4(int list[],int n)
{
    int i;
    int sum,maxsum;
    sum = maxsum = 0;
    for (i = 0;i<n;i++){
        sum +=list[i];
        if(sum>maxsum)
           maxsum = sum;
        else if(sum<0)
           sum = 0;
    }
    return maxsum;
}

int main()
{
    int n,i;
	scanf("%d",&n);
	int list[n];
	for(i=0;i<n;i++)
		scanf("%d",&list[i]);
	printf("%d",mss4(list,n));
    return 0;	
}
