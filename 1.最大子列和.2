#include<stdio.h>
int mss4(int list[],int n)
{
    int i,j;
    int sum,maxsum;
    sum = maxsum = 0;
    for (i = 0;i<n;i++){
		sum = 0;
        for (j=i;j<n;j++){
			sum +=list[j];
            if(sum>maxsum)
            maxsum = sum;     
           }  
    }
    if(maxsum<0)
           maxsum = 0;
    return maxsum;
}

int main()
{
    int n,i;
	scanf("%d",&n);
	int list[n]; 
	for(i=0;i<n;i++)
		scanf("%d",&list[i]);
	printf("%d",mss4(list,n));
    return 0;	
}
